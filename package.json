{
  "name": "callsite-log",
  "version": "0.1.0",
  "description": "Log values with caller file and line number.",
  "license": "MIT",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --pool=threads",
    "dev": "vitest --pool=threads",
    "pack:dry-run": "npm pack --dry-run --cache ./.npm-cache",
    "publish:dry-run": "npm publish --dry-run --cache ./.npm-cache",
    "release:check": "npm run pack:dry-run && npm run publish:dry-run"
  },
  "keywords": [
    "logger",
    "debug",
    "callsite",
    "typescript"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
